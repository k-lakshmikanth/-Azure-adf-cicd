		"LS_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devadlsmvc.dfs.core.windows.net/"
		},
		"az_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://dev-mvc-kv.vault.azure.net/"
		},
		"default_properties_global_param1_value": {
			"type": "string",
			"defaultValue": "hello"
		}
	},
